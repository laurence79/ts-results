{
  "name": "@laurence79/ts-results",
  "version": "0.0.1",
  "description": "Result primitives for ts/js",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:cjs": "./node_modules/.bin/ttsc -p tsconfig.release.cjs.json",
    "build:esm": "./node_modules/.bin/ttsc -p tsconfig.release.esm.json",
    "build:watch": "./node_modules/.bin/ttsc -p tsconfig.json --watch",
    "clean": "rimraf lib",
    "lint": "./node_modules/.bin/eslint --cache 'src/**/*.ts' --quiet",
    "lint:watch": "./node_modules/.bin/esw --cache 'src/**/*.ts' --watch",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurence79/ts-results.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/laurence79/ts-array-extensions/issues"
  },
  "homepage": "https://github.com/laurence79/ts-array-extensions#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/ts-nameof": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-nameof": "^5.0.0",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2"
  }
}
